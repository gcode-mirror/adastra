<Window x:Class="WPF.ClassifyWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Title="Classification" Height="580" Width="600" Loaded="Window_Loaded"
    >
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition></RowDefinition>
            <RowDefinition Height="120"></RowDefinition>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*"/>
            <ColumnDefinition Width="*"/>
        </Grid.ColumnDefinitions>
        
        <ListBox Grid.Row="0" Grid.Column="0" Name="listModels" Padding="4,4,4,4" Margin="4,4,4,4"/>
        <ListBox Grid.Row="0" Grid.Column="1" Name="listActions" Padding="4,4,4,4" Margin="4,4,4,4"/>
        <Canvas Name="canv" Grid.Row="1" Grid.Column="0" Grid.ColumnSpan="2" Margin="4,4,4,4"></Canvas>
        
    </Grid>
</Window>
